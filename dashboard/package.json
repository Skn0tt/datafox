  "devDependencies": {
    "@next/bundle-analyzer": "12.0.8",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.16",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.43",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.1.7",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "3.8.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "ts-node": "10.7.0",
    "typescript": "^4.5.3"
  },
  "private": true

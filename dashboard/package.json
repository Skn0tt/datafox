  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "alpha",
    "@blitzjs/next": "alpha",
    "@blitzjs/rpc": "alpha",
    "@chakra-ui/icons": "1.1.7",
    "@chakra-ui/react": "1.8.8",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@prisma/client": "3.9.0",
    "blitz": "2.0.0-alpha.18",
    "chakra-react-select": "3.3.1",
    "date-fns": "2.28.0",
    "final-form": "4.20.7",
    "framer-motion": "6.3.3",
    "next": "12.1.1",
    "prisma": "3.9.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-final-form": "6.5.9",
    "react-syntax-highlighter": "15.5.0",
    "zod": "3.10.1"
